/* pages/test/test.wxss */

.tenst {
    width: 750rpx;
    height: 100%;
    display: flex;
    flex-flow: column wrap;
    justify-content: center;
    align-items: center;
}

.twice {
    width: 400rpx;
    height: 400rpx;
    border: 1rpx solid #000;
    border-radius: 200rpx;
    position: relative;
    animation: rot 1s linear infinite;
}

@keyframes rot {
    0% {
        transform: rotateZ(0deg)
    }
    50% {
        transform: rotateZ(180deg)
    }
    100% {
        transform: rotateZ(360deg)
    }
    
}

.blank {
    position: absolute;
    width: 200rpx;
    height: 400rpx;
    border-radius: 200rpx 0 0 200rpx;
    background-color: #000;
}

.left {
    position: absolute;
    top: 0rpx;
    left: 200rpx;
    width: 100rpx;
    height: 200rpx;
    border-radius: 0 100rpx 100rpx 0;
    border-left: none;
    background-color: #000;
}

.right {
    position: absolute;
    bottom: 0rpx;
    right: 200rpx;
    width: 100rpx;
    height: 200rpx;
    border-radius: 100rpx 0 0 100rpx;
    border-right: none;
    background-color: #fff;
}

.ears-left {
    position: absolute;
    left: -25rpx;
    top: 75rpx;
    width: 50rpx;
    height: 50rpx;
    border: 1rpx solid #000;
    border-radius: 25rpx;
    background-color: #fff;
}

.ears-right {
    position: absolute;
    right: -25rpx;
    top: 75rpx;
    width: 50rpx;
    height: 50rpx;
    border: 1rpx solid #000;
    border-radius: 25rpx;
    background-color: #000;
}